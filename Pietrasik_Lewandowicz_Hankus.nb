Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{"1", "-", 
    RowBox[{"x", "^", "2"}]}]}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"calculateCoefficient", "[", 
     RowBox[{"f_", ",", "n_", ",", "x_", ",", "period_", ",", "type_"}], 
     "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "integrand", "}"}], ",", 
      RowBox[{
       RowBox[{"integrand", "=", 
        RowBox[{"f", "*", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"type", "==", "\"\<Cos\>\""}], ",", 
           RowBox[{"Cos", "[", 
            RowBox[{"2", " ", "Pi", " ", "n", " ", 
             RowBox[{"x", "/", "period"}]}], "]"}], ",", 
           RowBox[{"Sin", "[", 
            RowBox[{"2", " ", "Pi", " ", "n", " ", 
             RowBox[{"x", "/", "period"}]}], "]"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "/", "period"}], ")"}], " ", 
        RowBox[{"Integrate", "[", 
         RowBox[{"integrand", ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{
             RowBox[{"-", "period"}], "/", "2"}], ",", 
            RowBox[{"period", "/", "2"}]}], "}"}]}], "]"}]}]}]}], "]"}]}], 
   ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"fourierCoefficients", "[", 
