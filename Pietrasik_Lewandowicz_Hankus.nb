Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Lagrange", "[", "points_List", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "w", ",", " ", "n", ",", "  ", "i", ",", " ", "phi", ",", " ", "k"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"w", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"n", " ", "=", " ", 
       RowBox[{"Length", "[", "points", "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", "n"}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"phi", " ", "=", " ", "1"}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"k", " ", "=", " ", "1"}], ",", " ", 
           RowBox[{"k", " ", "<=", " ", "n"}], ",", " ", 
           RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"k", "!=", "i"}], ",", " ", 
              RowBox[{"phi", " ", "=", 
               RowBox[{"phi", "*", " ", 
                FractionBox[
                 RowBox[{"x", "-", 
                  RowBox[{"points", "\[LeftDoubleBracket]", 
                   RowBox[{"k", ",", "1"}], "\[RightDoubleBracket]"}]}], 
                 RowBox[{
                  RowBox[{"points", "\[LeftDoubleBracket]", 
                   RowBox[{"i", ",", "1"}], "\[RightDoubleBracket]"}], "-", 
                  RowBox[{"points", "\[LeftDoubleBracket]", 
                   RowBox[{"k", ",", "1"}], 
                   "\[RightDoubleBracket]"}]}]]}]}]}], "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"w", "=", " ", 
          RowBox[{"w", " ", "+", " ", 
           RowBox[{
            RowBox[{"points", "\[LeftDoubleBracket]", 
             RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}], "*", 
            "phi"}]}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "w", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"points", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
