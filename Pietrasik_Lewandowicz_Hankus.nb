    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a0", ",", "an", ",", "bn"}], "}"}], ",", 
      RowBox[{
       RowBox[{"a0", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "/", "period"}], ")"}], " ", 
         RowBox[{"Integrate", "[", 
          RowBox[{"f", ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", 
             RowBox[{
              RowBox[{"-", "period"}], "/", "2"}], ",", 
             RowBox[{"period", "/", "2"}]}], "}"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"an", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"calculateCoefficient", "[", 
           RowBox[{
           "f", ",", "m", ",", "x", ",", "period", ",", "\"\<Cos\>\""}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"m", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"bn", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"calculateCoefficient", "[", 
           RowBox[{
           "f", ",", "m", ",", "x", ",", "period", ",", "\"\<Sin\>\""}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"m", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", "a0", "}"}], "~", "Join", "~", "an", "~", "Join", "~", 
        "bn"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"fourierSeries", "[", 
     RowBox[{"x_", ",", "n_", ",", "period_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
